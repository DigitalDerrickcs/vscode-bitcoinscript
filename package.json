{
    "name": "bsl",
    "displayName": "Bitcoin Scripting Language",
    "description": "Syntax highlighting for Bitcoin Script",
    "version": "0.0.7",
    "publisher": "shade",
    "homepage": "https://github.com/shade/bsl",
    "repository": {
        "type": "git",
        "url": "https://github.com/shade/bsl"
    },
    "bugs": {
        "url": "https://github.com/shade/bsl/issues"
    },
    "license": "MIT",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "bsl",
                "aliases": [
                    "Bitcoin Script",
                    "bsl"
                ],
                "extensions": [
                    ".bs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bsl",
                "scopeName": "source",
                "path": "./syntaxes/bsl.tmLanguage.json"
            }
        ]
    }
}