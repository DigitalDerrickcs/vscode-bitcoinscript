{
    "name": "bsl",
    "displayName": "BSL",
    "description": "Formatting for Bitcoin Script",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "bsl",
            "aliases": ["Bitcoin Script", "bsl"],
