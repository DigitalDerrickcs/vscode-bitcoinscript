{
    "name": "bsl",
    "displayName": "Bitcoin Scripting Language",
    "description": "Syntax highlighting for Bitcoin Script",
    "version": "0.0.9",
    "publisher": "shade",
    "homepage": "https://github.com/shade/bsl",
    "repository": {
        "type": "git",
        "url": "https://github.com/shade/bsl"
    },
    "bugs": {
        "url": "https://github.com/shade/bsl/issues"
    },
    "license": "MIT",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:bitcoinscript"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [
            {
                "id": "bitcoinscript",
                "aliases": [
                    "Bitcoin Scripting Language",
                    "Bitcoin Script",
                    "Bitcoin",
                    "bitcoinscript",
                    "bsl",
                    "bs"
                ],
                "extensions": [
                    ".bs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bitcoinscript",
                "scopeName": "source",
                "path": "./syntaxes/bitcoinscript.json"
            }
        ]
    },
    "dependencies": {
        "@types/node": "^14.0.23"
    },
    "devDependencies": {
        "vscode": "^1.1.37"
    }
}
